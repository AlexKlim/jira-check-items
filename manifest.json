{
  "name": "Jira Check Items",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Check item for jira",
  "options_page": "options.html",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://jira.sophiaapp.com/browse/*"],
      "js": ["jquery-1.8.2.min.js", "main.js", "script.js"],
      "css": ["style.css"]
    }
  ],
  "browser_action": {
    "default_popup": "main.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "background",
    "storage",
    "https://jira.sophiaapp.com/",
    "unlimitedStorage"
  ]
}
